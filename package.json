{
  "name": "my-nfts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.ts",
    "deploy:testnet": "hardhat run scripts/deploy.ts --network evmFlowTestnet",
    "deploy:mainnet": "hardhat run scripts/deploy.ts --network evmFlowMainnet",
    "mint": "hardhat run scripts/mint.ts",
    "mint:testnet": "hardhat run scripts/mint.ts --network evmFlowTestnet",
    "mint:mainnet": "hardhat run scripts/mint.ts --network evmFlowMainnet",
    "mint:testnet:nft": "hardhat mint --network evmFlowTestnet --symbol 'VNFT' --count",
    "mint:mainnet:nft": "hardhat mint --network evmFlowMainnet --symbol 'KNFT' --count",
    "verify:testnet": "NETWORK=evmFlowTestnet SYMBOL=VOK ts-node scripts/verify.ts",
    "verify:mainnet": "NETWORK=evmFlowMainnet SYMBOL=VOK ts-node scripts/verify.ts",
    "verify-metadata:testnet": "hardhat verify-metadata --network evmFlowTestnet --symbol",
    "verify-metadata:mainnet": "hardhat verify-metadata --network evmFlowMainnet --symbol",
    "set-uri": "hardhat run scripts/set-contract-uri.ts",
    "set-uri:testnet": "hardhat set-uri --network evmFlowTestnet --symbol VOK --uri https://raw.githubusercontent.com/bthaile/nft-collections/refs/heads/main/metadata/VolcanicWonder.json",
    "set-uri:mainnet": "hardhat set-uri --network evmFlowMainnet --symbol VOK --uri https://raw.githubusercontent.com/bthaile/nft-collections/refs/heads/main/metadata/VolcanicWonder.json",
    "set-uri:nft": "hardhat set-uri --network evmFlowMainnet --symbol VOK --uri https://raw.githubusercontent.com/bthaile/nft-collections/refs/heads/main/metadata/VolcanicWonder.json",
    "verify:nft": "hardhat verify-contract --network evmFlowMainnet --symbol MNFT",
    "deploy:testnet:NFT": "hardhat deploy --network evmFlowTestnet --symbol VOK --name \"Volcanic NFT\"",
    "deploy:mainnet:NFT": "hardhat deploy --network evmFlowMainnet --symbol VOK --name \"Volcanic NFT\"",
    "transfer:testnet": "hardhat transfer --network evmFlowTestnet",
    "transfer:mainnet": "hardhat transfer --network evmFlowMainnet",
    "dev": "cd web && pnpm run dev -- --port 5000",
    "build": "node vercel-build.js",
    "preview": "cd web && pnpm run preview",
    "batch-transfer:mainnet": "hardhat batch-transfer --network evmFlowMainnet --symbol MNFT --to 0x0000000000000000000000021403a2acbc78d508 --start 5 --count 5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
